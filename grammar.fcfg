% start S

S[SEM = <?subj(?vp)>] -> NP[SEM=?subj] VP[SEM=?vp]
VP[SEM=?v] -> IV[SEM=?v]
NP[SEM=<?det(?n)>] -> Det[SEM=?det] N[SEM=?n]
NP[SEM=<?pn>] -> PN[SEM=?pn]

Det[SEM=<\Q P.exists x.(Q(x) & P(x))>] -> 'a'
Det[SEM=<\Q P.all x.(Q(x) -> P(x))>] -> 'every'
N[SEM=<\x.student(x)>] -> 'student'
PN[SEM=<\x.name(x)>] -> 'John'
IV[SEM=<\x.eat(x)>] -> 'eats'
